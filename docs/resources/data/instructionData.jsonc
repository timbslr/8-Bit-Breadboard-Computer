    {
      "name": "add",
      "mnemonic": "add",
      "type": "REAL",
      "operands": ["reg"],
      "requiresFlag": false,
      "opcode": "110100RR",
      "microinstructions": [
        ["OE_PC_H", "IE_MAR_H"], //          ──┐
        ["OE_PC_L", "IE_MAR_L"], //            ├──> Fetch Opcode
        ["MEM_EN_IO", "IE_IR", "INC_PC"], // ──┘
        ["ALU_SRC_ARI", "ALU_AOP_ADD", "OE_ALU", "IE_<reg>", "IE_F"], //calculate addition and output to register (works in one step because addition and subtraction is fast)
        ["RSC"]
      ],
      "group": "ALU",
      "indexInGroup": 0,
      "shortDescription": "<reg> = A + TMP",
      "longDescription": "The ADD instruction computes an addition on the A and B registers. These two operands are fixed and cannot be changed. What can be changed is the register in which the result is saved, it is specified as an operand of the instruction. After computation, the corresponding flags are set.",
      "examples": [
        {
          "prerequisites": [
            { "register": "A", "value": 40 },
            { "register": "TMP", "value": 2 }
          ],
          "code": ["add B"],
          "result": [{ "register": "B", "value": 42 }]
        },
        {
          "prerequisites": [
            { "register": "A", "value": 2 },
            { "register": "TMP", "value": -4 }
          ],
          "code": ["add X"],
          "result": [{ "register": "X", "value": -2 }]
        },
        {
          "prerequisites": [
            { "register": "A", "value": -42 },
            { "register": "TMP", "value": -8 }
          ],
          "code": ["add A"],
          "result": [{ "register": "A", "value": -50 }]
        }
      ]
    },
    {
      "name": "add with immediate",
      "mnemonic": "addi",
      "type": "PSEUDO",
